public class ff {

    public static void main(String[] args) {
        int[] a= {10,20,30,40,};
        int t=50;
        int n= a.length;
        for(int i = 0;i<= n-1;i++)
        {
            for(int j = i+1 ;j<= n-1;j++) 
            if (a[i]+a[j] == t){
                System.out.println("index: "+ a[i]+ " and " + a[j] );
            }
            else {
                System.out.println("not available");
            }
        }
    }
}
